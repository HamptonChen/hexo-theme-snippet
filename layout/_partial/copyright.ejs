<div class="copyright">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <%- partial('_partial/busuanzi') %>
            </div>
            <div class="col-sm-12">
                <span>Copyright &copy; <%= theme.since %>
                </span> |
                <span>
                    Powered by <a href="//hexo.io" class="copyright-links" target="_blank" rel="nofollow">Hexo</a>
                </span> |
                <span>
                    Theme by <a href="//github.com/shenliyang/hexo-theme-snippet.git" class="copyright-links" target="_blank" rel="nofollow">Snippet</a>
                </span>
                <% if(theme.icp_id){ %>
                    <span> | </span>
                    <span>
                        <%= theme.icp_id %>
                    </span>
                <% } %>
                <% if(theme.gongan_beian.enable){ %>
                    <span> | </span>
                    <a target="_blank" href="http://www.beian.gov.cn/portal/registerSystemInfo?recordcode=35020302033449">
                        <img src="<%= theme.gongan_beian.icon %>"/>
                        <span><%= theme.gongan_beian.id %></span>
                    </a>
                <% } %>         
            </div>
        </div>
    </div>
</div>

<%- partial('_partial/script') %>
